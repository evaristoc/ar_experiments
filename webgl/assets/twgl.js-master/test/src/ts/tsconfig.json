{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "outDir": "dist",
        "sourceMap": true,
        "declaration": true,
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "downlevelIteration": true,
        "strict": true,
        "rootDir": ".",
        "baseUrl": "."
    },
    "include": [
        "./*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}
