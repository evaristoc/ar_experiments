{
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "rules": {
        "object-literal-sort-keys": false,
        "trailing-comma": false,
        "no-console": [true, "log", "error"],
        "ordered-imports": false,
        "no-bitwise": false,
        "variable-name": false,
        "prefer-for-of": false,
        "interface-name": false,
        "no-unused-expression": true
    }
}
